{
  "name": "@didinsino/telegram-bot",
  "version": "2.0.1",
  "description": "Simple and lightweight NodeJS library for creating Telegram bots",
  "main": "dist/index.js",
  "type": "commonjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && rm -rf dist/types.js",
    "build:watch": "tsc --watch",
    "publish": "npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/didinsino/telegram-bot.git"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "keywords": [
    "telegram",
    "bot",
    "nodejs",
    "javascript",
    "typescript"
  ],
  "author": "Didin Sino",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/didinsino/telegram-bot/issues"
  },
  "homepage": "https://github.com/didinsino/telegram-bot#readme",
  "devDependencies": {
    "@types/lodash": "^4.14.192",
    "@types/node": "^18.15.11",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "lodash": "^4.17.21",
    "promise-events": "^0.2.4"
  }
}
